}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
bat "C:\Program Files\Maven\apache-maven-3.9.4\bin\mvn compile"
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}
